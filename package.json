{
  "name": "privatebin-cli",
  "version": "1.0.0",
  "homepage": "https://github.com/pixelfactoryio/privatebin-cli",
  "description": "a PrivateBin client written in typescript",
  "author": "Amine Benseddik <amine@pixelfactory.io>",
  "keywords": [
    "privatebin",
    "zerobin",
    "paste",
    "cli",
    "crypto",
    "aes256",
    "zlib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelfactoryio/privatebin-cli.git"
  },
  "readme": "https://github.com/pixelfactoryio/privatebin-cli#readme",
  "bugs": {
    "url": "https://github.com/pixelfactoryio/privatebin-cli/issues"
  },
  "bin": {
    "privatebin": "build/src/bin/privatebin.js"
  },
  "main": "build/src/lib/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lintfix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "tap -J --timeout=300",
    "posttest": "tap --coverage-report=text-lcov > coverage.out",
    "dev": "tsc --build --watch",
    "clean": "tsc --build --clean",
    "build": "npm run clean && tsc --build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bs58": "^4.0.1",
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "esm": "^3.2.25",
    "pako": "^1.0.11",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/node": "^14.0.4",
    "@types/pako": "^1.0.1",
    "@types/tap": "^14.10.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "tap": "^14.10.7",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
